{
  "name": "@exnest/extended-nest",
  "version": "0.2.1",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "run": "ts-node-dev --respawn -- ./src/index.ts",
    "build": "tsc -b",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "hopedevelopment",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExNest/ExtendedNest.git"
  },
  "bugs": {
    "url": "https://github.com/ExNest/ExtendedNest/issues"
  },
  "homepage": "https://github.com/ExNest/ExtendedNest#readme",
  "dependencies": {
    "@js-joda/core": "^5.6.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add . && echo 'Success add staging files'",
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,ts}": ["prettier --write"]
  }
}
